    <tr>
        <td valign="top" class="imageContent">
            <img src="http://placekitten.com/g/1120/800" width="560" class="flexibleImage" style="max-width:560px;" />
        </td>
    </tr>
</table>
<!-- // CONTENT TABLE -->
{{> module_end}}
